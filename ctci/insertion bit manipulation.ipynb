   "source": [
    "N = int(raw_input(), 2)\n",
    "M = int(raw_input(), 2)\n",
    "i, j = map(int, raw_input().strip().split(' '))\n",
    "\n",
    "count = j-i\n",
    "num = 0\n",
    "while count >= 0:\n",
    "    num += 2**count\n",
    "    count -= 1\n",
    "\n",
    "print bin((N & ~(num << i)) + (M << i))"
   ]
